generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

model users {
  id        BigInt   @id @default(autoincrement())
  name      String   @db.VarChar
  password  String   @db.VarChar
  createdAt DateTime
}

model notes {
  id        BigInt   @id @default(autoincrement())
  createdAt DateTime
}
